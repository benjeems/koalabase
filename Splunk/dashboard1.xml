<form>
  <label>Koalabase</label>
  <fieldset submitButton="false" autoRun="true">
    <input type="dropdown" token="Earliest_Year" searchWhenChanged="true">
      <label>Earliest year (for cluster map only)</label>
      <default>1996</default>
      <initialValue>1996</initialValue>
      <fieldForLabel>Earliest_Year</fieldForLabel>
      <fieldForValue>Year</fieldForValue>
      <search>
        <query>index=koalabase | table Year | dedup Year</query>
        <earliest>0</earliest>
        <latest></latest>
      </search>
    </input>
    <input type="dropdown" token="Latest_Year" searchWhenChanged="true">
      <label>Latest year (for cluster map only)</label>
      <default>2017</default>
      <initialValue>2017</initialValue>
      <fieldForLabel>Latest_Year</fieldForLabel>
      <fieldForValue>Year</fieldForValue>
      <search>
        <query>index=koalabase | table Year | dedup Year</query>
        <earliest>0</earliest>
        <latest></latest>
      </search>
    </input>
    <input type="dropdown" token="Input_LGA" searchWhenChanged="true">
      <label>Local Government Authority</label>
      <fieldForLabel>Input_LGA</fieldForLabel>
      <fieldForValue>Koala LGA</fieldForValue>
      <search>
        <query>index=koalabase | table "Koala LGA" | dedup "Koala LGA" | sort "Koala LGA"</query>
        <earliest>0</earliest>
        <latest></latest>
      </search>
      <choice value="*">No Filter</choice>
      <initialValue>*</initialValue>
      <default>*</default>
    </input>
    <input type="dropdown" token="Input_cause" searchWhenChanged="true">
      <label>Cause</label>
      <choice value="*">No Filter</choice>
      <choice value="&quot;Vehicle Hit&quot;=TRUE">Vehicle Hit</choice>
      <choice value="&quot;Caused By Dog&quot;=TRUE">Dog</choice>
      <choice value="Fall=TRUE">Fall</choice>
      <choice value="NOT (&quot;Vehicle Hit&quot;=TRUE OR &quot;Caused By Dog&quot;=TRUE OR Fall=TRUE)">Other Causes</choice>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
    <input type="dropdown" token="Input_Gender" searchWhenChanged="true">
      <label>Adult Gender</label>
      <choice value="*">No Filter</choice>
      <choice value="&quot;Adult Gender&quot;=&quot;Male&quot;">Male</choice>
      <choice value="&quot;Adult Gender&quot;=&quot;Female&quot;">Female</choice>
      <choice value="&quot;Adult Gender&quot;=&quot;Unknown&quot;">Unknown</choice>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
    <input type="dropdown" token="Input_EyeDisease" searchWhenChanged="true">
      <label>Eye Disease</label>
      <choice value="*">No Filter</choice>
      <choice value="Conjunctivitis=TRUE">Conjunctivitis</choice>
      <choice value="Cystitis=TRUE">Cystitis</choice>
      <choice value="Conjunctivitis=TRUE OR Cystitis=TRUE">Either</choice>
      <choice value="Conjunctivitis=TRUE AND Cystitis=TRUE">Both</choice>
      <choice value="Conjunctivitis= FALSE AND Cystitis=FALSE">Neither</choice>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
    <input type="radio" token="InputWasted" searchWhenChanged="true">
      <label>Koala Wasted</label>
      <choice value="*">No Filter</choice>
      <choice value="Wasted=TRUE">Yes</choice>
      <choice value="Wasted=FALSE">No</choice>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
    <input type="radio" token="Input_YoungPresent" searchWhenChanged="true">
      <label>Young Present ?</label>
      <choice value="*">No Filter</choice>
      <choice value="&quot;Young Present&quot;=TRUE">Yes</choice>
      <choice value="&quot;Young Present&quot;=FALSE">No</choice>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
    <input type="radio" token="Input_alive" searchWhenChanged="true">
      <label>Koala found Alive?</label>
      <choice value="*">No Filter</choice>
      <choice value="Dead=FALSE">Alive</choice>
      <choice value="Dead=TRUE">Dead</choice>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
    <input type="radio" token="Input_Orphaned" searchWhenChanged="true">
      <label>Koala Orphaned?</label>
      <choice value="*">No Filter</choice>
      <choice value="Orphaned=TRUE">Yes</choice>
      <choice value="Orphaned=FALSE">No</choice>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
    <input type="radio" token="Input_Released" searchWhenChanged="true">
      <label>Koala was Released ?</label>
      <choice value="*">No filter</choice>
      <choice value="&quot;Release Date&quot;=&quot;*&quot;">Yes</choice>
      <choice value="NOT &quot;Release Date&quot;=&quot;*&quot;">No</choice>
      <default>*</default>
      <initialValue>*</initialValue>
    </input>
  </fieldset>
  <row>
    <panel>
      <title>Geospatial Clusters</title>
      <viz type="viz_clustermap.clustermap">
        <search>
          <query>index=koala* $Input_Released$ $Input_Orphaned$ $Input_alive$ $InputWasted$ $Input_YoungPresent$ $Input_Gender$ $Input_EyeDisease$ $Input_cause$ LATITUDE=* LONGITUDE=* Year&gt;=$Earliest_Year$ Year&lt;=$Latest_Year$ "Koala LGA"="$Input_LGA$" | geostats   latfield=LATITUDE longfield=LONGITUDE count</query>
          <earliest>0</earliest>
          <latest></latest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="drilldown">none</option>
        <option name="height">822</option>
        <option name="trellis.enabled">0</option>
        <option name="trellis.scales.shared">1</option>
        <option name="trellis.size">medium</option>
        <option name="viz_clustermap.clustermap.lat">-26.1834</option>
        <option name="viz_clustermap.clustermap.lng">152.6657</option>
        <option name="viz_clustermap.clustermap.markerColor1">#008cff</option>
        <option name="viz_clustermap.clustermap.markerColor2">#ffbf00</option>
        <option name="viz_clustermap.clustermap.markerColor3">#ff0000</option>
        <option name="viz_clustermap.clustermap.markerColor4">#ff00ed</option>
        <option name="viz_clustermap.clustermap.maxClusters">805</option>
        <option name="viz_clustermap.clustermap.numberFormat_min_0">0.[0]</option>
        <option name="viz_clustermap.clustermap.numberFormat_min_1000">1.1a</option>
        <option name="viz_clustermap.clustermap.numberFormat_min_10000">1a</option>
        <option name="viz_clustermap.clustermap.numberFormat_min_1000000">1.1a</option>
        <option name="viz_clustermap.clustermap.size">51</option>
        <option name="viz_clustermap.clustermap.tiles">light</option>
        <option name="viz_clustermap.clustermap.tiles_max_zoom">111</option>
        <option name="viz_clustermap.clustermap.tiles_min_zoom">1</option>
        <option name="viz_clustermap.clustermap.zoom">7</option>
      </viz>
    </panel>
  </row>
